# 샘플 통과 후 제출했는데 정확성 테스트는 18개 다 통과했는데
# 효율성 테스트 세개중에 한개가 실패했다...ㅠ__ㅠ...
# 어떻게 고쳐야 할까. 뭐때문에 효율성에서 실패한거지..?
****************************************************
def solution(A, B): 
    win = 0
    A.sort(reverse=True)
    B.sort(reverse=True)
    for i in range(len(A)):
        for j in range(len(B)):
            if(A[i]>=B[j]):
                continue
            else:
                win += 1
                del B[j]
                break;
    return win 
****************************************************

# 그런데 이렇게 바꾸니까 됐다 ; 

****************************************************
def solution(A, B): 
    win = 0
    sortA = sorted(A)
    sortB = sorted(B)
    for i in range(len(sortA)):
        for j in range(len(sortB)):
            if(sortB[j]>sortA[i]):
                win += 1
                del sortB[j]
                break;
    return win 
***************************************************

//c++버전

***************************************************
#include <string>
#include <vector>
#include <algorithm>
using namespace std;

int solution(vector<int>A, vector<int>B){
	int win = 0;
	int j = 0;
	sort(A.begin(), A.end());
	sort(B.begin(), B.end());
	for(int i = 0; i<B.size(); i++){
		if(A[j]<B[i]){
			win++;
			j++;
		}
	}
	return win;
}
****************************************************

